{
    "files.autoSave": "onFocusChange",
    "editor.lineNumbers": "on",
    "editor.wordWrap": "off",
    "editor.rulers": [
        100
    ],
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelZoom": true,
    "debug.inlineValues": true,
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.easymotion": true,
    "vim.leader": "e",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
    "vim.easymotionMarkerFontFamily": "SauceCodePro Nerd Font",
    "vim.easymotionMarkerYOffset": 0,
    "vim.easymotionMarkerBackgroundColor": "Black",
    "vim.easymotionMarkerForegroundColorOneChar": "Yellow",
    "vim.easymotionMarkerForegroundColorTwoChar": "Yellow",
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.easymotionMarkerHeight": 18,
    "window.zoomLevel": -1,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["s"],
            "after": ["e", "e", "s"]
        },
        {
            "before": [" "],
            "after": ["/"]
        }
    ],
    "todo-tree.defaultHighlight": {
        "icon": "alert",
        "type": "line",
        "foreground": "yellow",
        "background": "white",
        "iconColour": "blue"
    },
    "todo-tree.customHighlight": {
        "TODO": {
            "icon": "check",
            "type": "line"
        },
        "FIXME": {
            "foreground": "red",
            "iconColour": "yellow"
        }
    }
}
