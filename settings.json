{
    "vim.disableAnnoyingNeovimMessage": true,
    "files.autoSave": "afterDelay",
    "editor.lineNumbers": "on",
    "editor.wordWrap": "off",
    "editor.rulers": [
        100
    ],
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelZoom": true,
    "debug.inlineValues": true,
    "markdown.preview.scrollPreviewWithEditorSelection": false,
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.cmdLineInitialColon": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.easymotion": true,
    "vim.leader": "e",
    "vim.otherModesKeyBindings": [
        {
            "before": ["s"],
            "after": ["e", "e", "s"]
        },
        {
            "before": [" "],
            "after": ["/"]
        }
    ],
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
    "vim.easymotionMarkerFontFamily": "SauceCodePro Nerd Font",
    "vim.easymotionMarkerYOffset": 0,
    "vim.easymotionMarkerBackgroundColor": "Black",
    "vim.easymotionMarkerForegroundColorOneChar": "Yellow",
    "vim.easymotionMarkerForegroundColorTwoChar": "Yellow",
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.easymotionMarkerHeight": 18
}
