{
    "files.autoSave": "onFocusChange",
    "editor.lineNumbers": "on",
    "editor.wordWrap": "off",
    "editor.rulers": [
        100
    ],
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelZoom": true,
    "debug.inlineValues": true,
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.easymotion": true,
    "vim.leader": "e",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
    "vim.easymotionMarkerFontFamily": "SauceCodePro Nerd Font",
    "vim.easymotionMarkerYOffset": 0,
    "vim.easymotionMarkerBackgroundColor": "Black",
    "vim.easymotionMarkerForegroundColorOneChar": "Yellow",
    "vim.easymotionMarkerForegroundColorTwoChar": "Yellow",
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.easymotionMarkerHeight": 18,
    "window.zoomLevel": -1,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["s"],
            "after": ["e", "e", "s"]
        },
        {
            "before": [" "],
            "after": ["/"]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
    ],
    // "vim.statusBarColorControl": true,
    // "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
    // "vim.statusBarColors.insert": "#BF616A",
    // "vim.statusBarColors.visual": "#B48EAD",
    // "vim.statusBarColors.visualline": "#B48EAD",
    // "vim.statusBarColors.visualblock": "#A3BE8C",
    // "vim.statusBarColors.replace": "#D08770",
    "todo-tree.defaultHighlight": {
        "icon": "alert",
        "type": "line",
        "foreground": "yellow",
        "background": "white",
        "iconColour": "yellow"
    },
    "todo-tree.customHighlight": {
        "TODO": {
            "icon": "check",
            "type": "text",
        },
        "FIXME": {
            "foreground": "red",
            "iconColour": "yellow",
            "type": "text",
        },
        "QUESTION": {
            "icon": "pin",
            "type": "text",
            "iconColour": "red"
        },
    },
    "todo-tree.tags": [
        "TODO",
        "FIXME",
        "QUESTION",
    ],
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
}
